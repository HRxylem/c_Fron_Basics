#include <stdio.h>
int main(){
    int a, b;
    printf("enter the number");
    scanf("%d %d", &a, &b);
    if ((a==b) &&  a!=b){
        printf("they can eat \n", a);
    }else  if((a>b) || a>=b){
        printf ("they can vote\n" ,b);
    } else if((a<b) || a<=b ){
        printf("they cannot eat and and vote\n");
    }
    return 0;
}
